    "eslint-config-airbnb-typescript": "^14.0.1",
    "firebase": "^9.2.0",
    "instantsearch.css": "^7.4.5",
    "next": "12.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.18.1",
    "react-instantsearch-dom": "^6.15.0",
